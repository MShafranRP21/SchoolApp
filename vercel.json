{
  "functions": {
    "api/index.php": {
      "runtime": "now-php@1.0.0"
    },

  },
  "routes": [
    { "src": "/(.*)", "dest": "/index.php" },
     {
              "src": "/(.*)",
              "dest": "index.js"
            }
  ],

  "version": 2,
          "builds": [
            {
              "src": "index.js",
              "use": "@vercel/node"
            }
          ],

  
}

